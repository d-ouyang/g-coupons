page{
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  font-family: PingFangSC,'微软雅黑', Arial, Helvetica, sans-serif;
}

text {
  line-height: 1.0;
}

input {
  border: 0;
  outline: none;
}

button::after{
  border: none;
}

.container {
  background-color: #fff;
} 

.no-wrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}


