page {
  background-color: #F5F6F7;
}

.container {
  background-color: transparent;
}

.tabbar{
  box-sizing: border-box;
  position: fixed;
  z-index: 100;
  width: 100%;
  height: 70rpx;
  background-color: #fff;
  border-bottom: 0.5px solid rgba(51,52,64,0.1);
}

.tab-item{
  box-sizing: border-box;
  position: absolute;
  width: 120rpx;
  height: 70rpx;
  line-height: 1.0;
  text-align: center;
  font-size: 28rpx;
  padding-top: 10rpx;
  font-weight: 500;
  color: #333440;
}

.left-148{
  left:148rpx;
}

.left-484{
  left:484rpx;
}

.right-148{
  right: 148rpx;
}

.active-color{
  color: #617FDF;
}

.active-bottom{
  position: absolute;
  width: 120rpx;
  height: 6rpx;
  background-color: #617FDF;
  border-radius: 4rpx;
  bottom: 0;
  transition: left 0.3s ease-out;
}

.coupons-box {
  background-color: transparent;
}

.coupons-list {
  box-sizing: border-box;
  padding: 118rpx 40rpx 88rpx;
  background-color:transparent;
}

.product {
  position: fixed;
  width: 100%;
  height: 88rpx;
  bottom: 0;
  left: 0;
  background-color: #E2E4E9;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  font-weight: 500;
}

.product-active {
  background-color: #617FDF;
}

.no-more {
  font-size: 26rpx;
  color: #79808A;
  text-align: center;
  line-height: 66rpx;
}